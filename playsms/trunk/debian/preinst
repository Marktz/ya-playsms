#!/bin/sh

set -e

if [ "$1" = "install" ]; then
     # save the original kannel confs
     # before we clobber them
     #

     kannelconf=/etc/kannel/kannel.conf
     if [ -e $kannelconf ]; then
        cp $kannelconf $kannelconf.1 || true
     fi

     kanneldefaults=/etc/default/kannel
     if [ -e $kanneldefaults ]; then
        cp $kanneldefaults $kanneldefaults.1 || true
     fi
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0

